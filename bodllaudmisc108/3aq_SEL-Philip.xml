                <head>53 Apostolorum Philippi et Iacobi.</head>
                <lg>
                    <l xml:id="bodllaudmisc108.3aq.1"><hi rend="touched-red">S</hi><sg type="ln"
                        />Eint <name type="person" ref="#bioPhilip0001"><choice>
                                <orig>phelipe</orig>
                                <reg>Phelipe</reg>
                            </choice></name> and <choice>
                            <orig>seint</orig>
                            <reg>Seint</reg>
                        </choice>
                        <name type="person" ref="#bioJames0001">Iacob</name><pc> &punctelev;
                            </pc><hi rend="touched-red">A</hi>po&slong;tles weren tweiȝe</l>
                    <l xml:id="bodllaudmisc108.3aq.2"><hi rend="touched-red">Þ</hi><sg type="ln"/>e
                        fo&rrot;me&slong;te dai þat comez in ma&ydot;<pc> &punctelev; </pc><hi
                            rend="touched-red">d</hi>eth hu&ydot; þoleden beiȝe</l>
                    <l xml:id="bodllaudmisc108.3aq.3"><hi rend="touched-red">A</hi><sg type="ln"
                        />ftur o&rrot;e louerdes passione<pc> &punctelev; </pc><hi
                            rend="touched-red">i</hi>n þe twentiþe ȝer</l>
                    <!-- folio 156b -->
                    <pb n="56v"/>
                    <cb n="1"/>
                    <l xml:id="bodllaudmisc108.3aq.4"><hi rend="touched-red">S</hi><sg type="ln"
                        />eint <name type="person" ref="#bioPhilip0001"><choice>
                                <orig>phelipe</orig>
                                <reg>Phelipe</reg>
                            </choice></name> p&rrot;echede in <name type="place" ref="#locScythia01"
                            >S&ydot;thie</name><pc> &punctelev; </pc><hi rend="touched-red">t</hi>o
                        men þat luþere weren</l>
                    <l xml:id="bodllaudmisc108.3aq.5"><hi rend="touched-red">Þ</hi><sg type="ln"/>is
                        men nomen <choice>
                            <orig>seint</orig>
                            <reg>Seint</reg>
                        </choice>
                        <name type="person" ref="#bioPhilip0001"><choice>
                                <orig>phelipe</orig>
                                <reg>Phelipe</reg>
                            </choice></name><pc> &punctelev; </pc><hi rend="touched-red">a</hi>nd to
                        heo&rrot;e Maumates<note type="enc" resp="#bioHO001">[al. maumet]</note> him
                        ladde</l>
                    <l xml:id="bodllaudmisc108.3aq.6"><hi rend="touched-red">A</hi><sg type="ln"/>nd
                        sone so he bi<sg/>fo&rrot;en hem cam <pc> &punctelev; </pc><hi
                            rend="touched-red">þ</hi>is maumates loude gradde</l>
                    <l xml:id="bodllaudmisc108.3aq.7"><hi rend="touched-red">F</hi><sg type="ln"
                        />u&ydot;r hu&ydot; bleuwen out of heo&rrot;e Mouþes<pc> &punctelev;
                            </pc><hi rend="touched-red">þ</hi>at þare<sg/>with hu&ydot;
                        b&rrot;ouȝten of daw</l>
                    <l xml:id="bodllaudmisc108.3aq.8"><hi rend="touched-red">Þ</hi><sg type="ln"/>e
                        p&rrot;inces sone of þe lond<pc> &punctelev; </pc><hi rend="touched-red"
                            >a</hi>nd tweie con&slong;tables of þe lawe</l>
                    <l xml:id="bodllaudmisc108.3aq.9"><hi rend="touched-red">A</hi><sg type="ln"/>nd
                        manie men þat þare<sg/>neiȝ weren<pc> &punctelev; </pc><hi
                            rend="touched-red">w</hi>el grete siknesse to heom nome</l>
                    <l xml:id="bodllaudmisc108.3aq.10"><hi rend="touched-red">S</hi><sg type="ln"
                        />ome fullen in<sg/>to þe d&rrot;opesie<pc> &punctelev; </pc><hi
                            rend="touched-red">a</hi>nd some meoseles bi<sg/>come</l>
                    <l xml:id="bodllaudmisc108.3aq.11"><hi rend="touched-red">M</hi><sg type="ln"
                        />uche reuþe was into al þat lond<pc> &punctelev; </pc><hi
                            rend="touched-red">o</hi>f þis misauenture</l>
                    <l xml:id="bodllaudmisc108.3aq.12"><hi rend="touched-red">W</hi><sg type="ln"
                        />ulle ȝe seide <choice>
                            <orig>seint</orig>
                            <reg>Seint</reg>
                        </choice>
                        <name type="person" ref="#bioPhilip0001"><choice>
                                <orig>phelip</orig>
                                <reg>Phelip</reg>
                            </choice></name> þo<pc> &punctelev; </pc><hi rend="touched-red">"</hi>mi
                        conseil alle i<sg/>hu&ydot;re</l>
                    <l xml:id="bodllaudmisc108.3aq.13"><hi rend="touched-red">A</hi><sg type="ln"
                        />nd ichulle þo&rrot;uȝ <choice>
                            <orig>godes</orig>
                            <reg>Godes</reg>
                        </choice> grace<pc> &punctelev; </pc><hi rend="touched-red">h</hi>elen eov
                        euer[e]ch<sg/>on.</l>
                    <l xml:id="bodllaudmisc108.3aq.14"><hi rend="touched-red">G</hi><sg type="ln"
                        />lad was þis folk of þat wo&rrot;d<pc> &punctelev; </pc><hi
                            rend="touched-red">a</hi>nd graunteden him a<sg/>non</l>
                    <l xml:id="bodllaudmisc108.3aq.15"><hi rend="touched-red">b</hi><sg type="ln"
                        />erez awei he seide þis foule wiȝt<pc> &punctelev; </pc><hi
                            rend="touched-red">þ</hi>at ȝe habbez to i<sg/>bede</l>
                    <l xml:id="bodllaudmisc108.3aq.16"><hi rend="touched-red">A</hi><sg type="ln"
                        />nd nimez þe fourme of þe holie croiz<pc> &punctelev; </pc><hi
                            rend="touched-red">a</hi>nd settez in his &slong;tude!</l>
                    <l xml:id="bodllaudmisc108.3aq.17"><hi rend="touched-red">Þ</hi><sg type="ln"
                        />at folk was fa&ydot;n of his bi<sg/>he&slong;te<pc> &punctelev; </pc><hi
                            rend="touched-red">i</hi><sg/>redie þare weren i<sg/>nowe</l>
                    <l xml:id="bodllaudmisc108.3aq.18"><hi rend="touched-red">A</hi><sg type="ln"
                        />ne rode hu&ydot; setten up on heiȝ<pc> &punctelev; </pc><hi
                            rend="touched-red">þ</hi>e maumates hu&ydot;
                        al<sg/>to<sg/>d&rrot;owe</l>
                    <l xml:id="bodllaudmisc108.3aq.19"><hi rend="touched-red">S</hi><sg type="ln"
                        />eint <name type="person" ref="#bioPhilip0001"><choice>
                                <orig>pheolip</orig>
                                <reg>Pheolip</reg>
                            </choice></name> þo&rrot;uȝ <choice>
                            <orig>godes</orig>
                            <reg>Godes</reg>
                        </choice> grace<pc> &punctelev; </pc><hi rend="touched-red">d</hi>&rrot;of
                        out bi<sg/>fo&rrot;e heo&rrot;e eiȝ</l>
                    <l xml:id="bodllaudmisc108.3aq.20">A foul deouel of þat on<pc> &punctelev;
                            </pc><hi rend="touched-red">þ</hi>at hu&ydot; alle i<sg/>seiȝe</l>
                    <l xml:id="bodllaudmisc108.3aq.21"><hi rend="touched-red">Þ</hi><sg type="ln"/>o
                        þis rode was up i<sg/>set<pc> &punctelev; </pc><hi rend="touched-red"
                        >a</hi>nd þe Maumet i<sg/>b&rrot;ouȝt to grounde</l>
                    <l xml:id="bodllaudmisc108.3aq.22"><hi rend="touched-red">A</hi><sg type="ln"
                        />lle þe sike men þat þare<sg/>neiȝ weren<pc> &punctelev; </pc><hi
                            rend="touched-red">b</hi>i<sg/>comen hole and sounde</l>
                    <l xml:id="bodllaudmisc108.3aq.23"><hi rend="touched-red">A</hi><sg type="ln"
                        />nd þe þreo men þat weren a<sg/>slawe<pc> &punctelev; </pc><hi
                            rend="touched-red">a</hi><sg/>ȝein to liue come</l>
                    <l xml:id="bodllaudmisc108.3aq.24"><hi rend="touched-red">M</hi><sg type="ln"
                        />uche was þe <choice>
                            <orig>Io&ydot;e</orig>
                            <reg>ioye</reg>
                        </choice> into al þat lond<pc> &punctelev; </pc><hi rend="touched-red"
                            >þ</hi>at men þare<sg/>of nome</l>
                    <l xml:id="bodllaudmisc108.3aq.25"><hi rend="touched-red">C</hi><sg type="ln"
                        />ri&slong;tinedom hu&ydot; guonne a<sg/>fongue<pc> &punctelev; </pc><hi
                            rend="touched-red">a</hi>nd Anouri þe swete rode</l>
                    <l xml:id="bodllaudmisc108.3aq.26"><hi rend="touched-red">A</hi><sg type="ln"
                        />nd bi<sg/>lieueden on him þat bouȝte us alle<pc> &punctelev; </pc><hi
                            rend="touched-red">þ</hi>are<sg/>on with is blode</l>
                    <l xml:id="bodllaudmisc108.3aq.27"><hi rend="touched-red">Þ</hi><sg type="ln"
                        />o&rrot;uȝ þe vertuwe of þis faire miracle<pc> &punctelev; </pc><hi
                            rend="touched-red">m</hi>uche folk of þe lond</l>
                    <l xml:id="bodllaudmisc108.3aq.28"><hi rend="touched-red">b</hi><sg type="ln"
                        />i<sg/>gan cri&slong;tine fo&rrot;<sg/>to beo<pc> &punctelev; </pc><hi
                            rend="touched-red">a</hi>nd þe riȝte bi<sg/>leue
                        onder<sg/>&slong;tonde</l>
                    <l xml:id="bodllaudmisc108.3aq.29"><hi rend="touched-red">A</hi><sg type="ln"
                        />fter þe wissingue of <choice>
                            <orig>seint</orig>
                            <reg>Seint</reg>
                        </choice>
                        <name type="person" ref="#bioPhilip0001"><choice>
                                <orig>phelip</orig>
                                <reg>Phelip</reg>
                            </choice></name><pc> &punctelev; </pc><hi rend="touched-red">a</hi>lle
                        heo&rrot;e lijf hu&ydot; guonne lede</l>
                    <l xml:id="bodllaudmisc108.3aq.30"><hi rend="touched-red">F</hi><sg type="ln"
                        />o&rrot; p&rrot;ophesie and riȝt<sg/>w[i]snesse<pc> &punctelev; </pc><hi
                            rend="touched-red">h</hi>u&ydot; heolden al þat he seide</l>
                    <l xml:id="bodllaudmisc108.3aq.31"><hi rend="touched-red">Þ</hi><sg type="ln"/>o
                            <name type="person" ref="#bioPhilip0001"><choice>
                                <orig>phelipe</orig>
                                <reg>Phelipe</reg>
                            </choice></name> was sixti ȝer<pc> &punctelev; </pc><hi
                            rend="touched-red">a</hi>nd seouene and twenti old</l>
                    <l xml:id="bodllaudmisc108.3aq.32"><hi rend="touched-red">I</hi><sg type="ln"/>n
                        pa&ydot;s he deide in his bed<pc> &punctelev; </pc><hi rend="touched-red"
                            >a</hi>se þe bok us hath i<sg/>told</l>
                    <l xml:id="bodllaudmisc108.3aq.33"><hi rend="touched-red">G</hi><sg type="ln"
                        />ret deol þat folk made þo<pc> &punctelev; </pc><hi rend="touched-red"
                            >a</hi>nd wel faire hu&ydot; him nom</l>
                    <l xml:id="bodllaudmisc108.3aq.34"><hi rend="touched-red">A</hi><sg type="ln"
                        />nd bureden him with gret honour<pc> &punctelev; </pc><hi
                            rend="touched-red">a</hi>se a swuch man to bi<sg/>com</l>
                </lg>
